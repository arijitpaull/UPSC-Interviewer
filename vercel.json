{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/session/init",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/session/track",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/session/report",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/session/delete",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/tts",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/stt",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/chat",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
}